{
  "name": "ui-components",
  "version": "1.0.0",
  "description": "UI components as Web Components",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "sideEffects": false,
  "scripts": {
    "prebuild": "lerna exec rimraf dist",
    "build": "lerna exec -- rollup -c ../../rollup.config.js",
    "test": "jest",
    "lint": "eslint '**/*.ts'",
    "lint:fix": "prettier-eslint $PWD/'**/*.ts' --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bjerkek/lerna-rollup-npm"
  },
  "keywords": [
    "lerna",
    "monorepo",
    "web-components",
    "jest"
  ],
  "author": "Bjerkek AS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bjerkek/lerna-rollup-npm/issues"
  },
  "homepage": "https://github.com/bjerkek/lerna-rollup-npm#readme",
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-transform-runtime": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-typescript": "7.12.7",
    "@rollup/plugin-babel": "5.2.2",
    "@rollup/plugin-node-resolve": "11.0.1",
    "@rollup/plugin-typescript": "8.1.0",
    "@types/jest": "26.0.20",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "@typescript-eslint/parser": "4.12.0",
    "babel-jest": "26.6.3",
    "eslint": "7.17.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "jest": "26.6.3",
    "lerna": "3.22.1",
    "prettier-eslint": "12.0.0",
    "prettier-eslint-cli": "5.0.0",
    "rimraf": "3.0.2",
    "rollup": "2.35.1",
    "rollup-plugin-string": "3.0.0",
    "rollup-plugin-terser": "7.0.2"
  },
  "dependencies": {
    "@babel/runtime": "7.12.5",
    "tslib": "2.0.3",
    "typescript": "4.1.3"
  },
  "jest": {
    "transform": {
      "\\.[jt]s?$": "babel-jest",
      "\\.html$": "<rootDir>/fileTransformer.js"
    }
  }
}
